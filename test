    while wait(1) do
        pcall(function()
            UpdateFps:Set(string.format("Fps : %.2f", fps))
            UpdatePing:Set(string.format("Ping : %.2f", game.Stats.Network.ServerStatsItem["Data Ping"]:GetValue()).." ms")
        end)
    end
end) -- Update Lable
